
<html>
    <head>
        <title>Lepton3.5</title>
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
		
    </head>
    <body>
	
		<nav class="navbar fixed-top navbar-dark bg-dark ">
		  <a class="navbar-brand" href="#">Sleep Tracker</a>
		</nav>
			<br>
			<br><br>
		<div class="container">
			
			<ul class="list-group" >
				<li class="list-group-item h4" >
					<div class="row">
							<div class="col-sm-10">
							  Files
							</div>
							
							<div class="col-sm-2 ">
							 <a class="btn btn btn-secondary  active" role="button"  href='/files/all' >Download All <i class="fa fa-download" aria-hidden="true"></i></a>
							</div>
					</div>
				</li>
				<% myFilesList.forEach(function(item,index){ %>
					<li class="list-group-item h4" >
						<div class="row">
							<div class="col-sm-11">
							  <%= item %> 
							</div>
							
							<div class="col-sm-1">
							  <a href=<%= "/files/" + item %> > <i class="fa fa-download" aria-hidden="true"></i> </a>
							</div>
						</div>
					</li>
				<% }) %>
			</ul>
		</div>
    </body>
</html>

